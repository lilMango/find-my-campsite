{
  "manifest_version": 2,

  "name": "Find My Camp Site",
  "description": "This extension allows user to find, watch, and (eventually book) a National Park campsite on recreation.gov",
  "version": "1.0",
  "background": {
        "scripts": [
            "background.js"
        ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Find My Campsite"
  },
  "permissions": [
    "activeTab",
    "http://www.recreation.gov/"
  ]
}